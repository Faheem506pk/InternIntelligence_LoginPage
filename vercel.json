{
    "routes": [
      { 
        "src": "/assets/(.*)\\.js$", 
        "headers": { "content-type": "application/javascript" },
        "dest": "/assets/$1.js" 
      },
      { 
        "src": "/assets/(.*)\\.css$", 
        "headers": { "content-type": "text/css" },
        "dest": "/assets/$1.css" 
      },
      { 
        "src": "/(.*)", 
        "dest": "/index.html" 
      }
    ]
  }